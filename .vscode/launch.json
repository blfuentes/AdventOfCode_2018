    "version": "0.2.0",
    "configurations": [
        {
            "name": "Current TS File (Server)",
            "type": "node",
            "request": "launch",
            "args": ["${workspaceFolder}/${relativeFile}"],
            "runtimeArgs": ["--nolazy", "-r", "ts-node/register"],
            "sourceMaps": true,
            "cwd": "${workspaceFolder}",
            "protocol": "inspector",
        },
        {
            "name": "BAK_Current TS File",
            "type": "node",
            "request": "launch",
            "runtimeArgs": [
                "--nolazy",
                "-r",
                "ts-node/register"
            ],
            "args": [
                "${workspaceRoot}/${relativeFile}"
            ],
            "protocol": "inspector",
        }
    ]
}